@using Teleoffice.Models


@{
    ViewData["Title"] = "Client";
}
<br>
<h3>Details of Professionals</h3>
<br>
<div class="row">
    <div class="col-sm-7">
        <div class="panel panel-default">
            <div class="panel-body">
                                @if (ViewBag.CallRate.Count > 0)
                                {
                                    <ul class="list-group">

                                        @foreach (var n in ViewBag.CallRate)
                                        {
                                            <li class="list-group-item call">
                                                <div class="row">
                                                    <div class="col-sm-10">
                                                      Name:   @n.FirstName @n.LastName<br>
                                                      Email:  @n.Email <br>
                                                      Contact No.  @n.Contact<br>
                                                      Fees( Charges per Hour):  Rs. @n.CallRate
                                                    </div>
                                                </div>
                                            </li>
                                        }
                                    </ul>
                                }
           </div>
     </div>
   </div>
</div>
